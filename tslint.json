{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "object-literal-sort-keys": [true, "ignore-case"],
    "quotemark": [
      false,
      "double"
    ],
    "no-var-requires": true
  },
  "rulesDirectory": []
}
